<%= render "shared/notifications" %>
<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>

<p id="search_field">
  <%= render 'books/index/search_field' %>
</p>

<div id="newBtnWrapper">
  <div id="newBtn" class="btn btn-primary">教材登録</div>
</div>

<div id="book_new_modal_modal">
  <%= render 'books/index/book_new_modal' %>
</div>

<div id="book_category_modal">
  <%= render 'books/book_category_modal' %>
</div>

<p><%= link_to "#{@user.name}さんの本棚", user_books_path(@user), method: :get, remote: true %></p>

<div id="book_field">
  <%= render 'books/index/book_field' %>
</div>

<div id="book_status_edit_modal"></div>

<%= javascript_pack_tag 'books/index' %>